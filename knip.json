{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/**/page.tsx",
    "src/app/**/layout.tsx",
    "src/app/**/route.ts",
    "src/app/**/loading.tsx",
    "src/app/**/error.tsx",
    "proxy.ts",
    "src/i18n.ts"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    ".github/**",
    "supabase/**",
    "docs/**",
    "scripts/**",
    "sanity/**",
    "**/*.d.ts",
    "**/*.stories.tsx",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx"
  ],
  "ignoreDependencies": [
    "@types/*",
    "@sanity/code-input",
    "@sanity/document-internationalization",
    "@sanity/vision",
    "leaflet",
    "react-leaflet",
    "react-aria",
    "react-stately",
    "use-debounce",
    "tailwindcss",
    "tw-animate-css",
    "postcss",
    "@portabletext/types",
    "motion"
  ],
  "ignoreExportsUsedInFile": true,
  "next": {
    "entry": [
      "src/app/**/page.tsx!",
      "src/app/**/layout.tsx!",
      "src/app/**/route.ts!",
      "proxy.ts!"
    ]
  }
}
